                           /\ entry' = [entry EXCEPT ![self] = [host_marker |-> hostMarker_PrivToP'[self],
                                                                orig |-> [saddr |-> pkt_PrivToP'[self].saddr, sport |-> pkt_PrivToP'[self].sport,
                                                                          daddr |-> pkt_PrivToP'[self].daddr, dport |-> pkt_PrivToP'[self].dport],
