    privtoPubMan2AddT: T := Append(T, entry);
    privtopubPkt: pkt := [saddr |->pkt.daddr, sport |-> pkt.dport,
                          daddr |-> N, dport |-> pkt.sport,
                          host_marker |-> hostMarker];
    SendQueue := Append(SendQueue, pkt);
  };
  privtopubMan2Ret: return;
}
