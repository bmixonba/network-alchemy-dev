                                 /\ ip_D' = [ip_D EXCEPT ![self] = conn_Dis'[self][1]]
                                 /\ host_Di' = [host_Di EXCEPT ![self] = conn_Dis'[self][2]]
                                 /\ PrintT(<< "Disconnect- Before:", host_Di'[self], ip_D'[self], Connections>>)
